{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"next,scss\" --prefix-colors \"magenta,yellow\" \"yarn next:dev\" \"yarn build:scss\"",
    "next:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:next": "eslint \"**/*.{ts,tsx}\" --cache",
    "lint": "yarn lint:next",
    "lint:scss": "stylelint \"**/*.{css,scss}\" --cache",
    "lint:fix": "yarn lint:next --fix && yarn lint:scss --fix",
    "build:scss": "typed-scss-modules \"core/components/**/*.scss\" pages -w --additionalData \"@import 'core/styles/_mixins';\""
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.0",
    "@types/fabric": "^4.5.12",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "date-fns-tz": "^1.3.7",
    "fabric": "4.3.0",
    "js-cookie": "^3.0.1",
    "next": "12.2.2",
    "noise-gate": "^0.0.3",
    "peerjs": "^1.4.7",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-datepicker": "^4.8.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-select": "^5.4.0",
    "sass": "^1.53.0",
    "socket.io-client": "^4.5.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/axios": "^0.14.0",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-color": "^3.0.6",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "18.0.6",
    "@types/react-select": "^5.0.1",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.2.2",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "stylelint": "^14.14.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "^4.3.0",
    "typed-scss-modules": "^7.0.1",
    "typescript": "4.7.4"
  }
}
